
public class Pessoa {
	public String nome;
	public String email;
	public String endereco;
	public String cpf;
	public String telefone;
	
	public Pessoa(String nome,String email, String endereco, String cpf,  String telefone) {
		this.nome = nome;
		this.email = email;
		this.endereco = endereco;
		this.telefone = telefone;
		this.cpf = cpf;
		this.telefone = telefone;
	}
	
}
